{
  "name": "__MSG_extensionName__",
  "short_name": "__MSG_extensionNameShort__",
  "description": "__MSG_extensionDescription__",
  "version": "1.0",
  "author": "Shaa3",
  "homepage_url": "https://github.com/Shaa3/nominers",
  "default_locale": "en",
  "manifest_version": 2,
  "incognito": "spanning",
  "icons": {
    "48": "assets/icons/icon_blocked.svg",
	"96": "assets/icons/icon_blocked.svg",
	"24": "assets/icons/icon_blocked.svg"
  },
  "developer": {
	"name": "Shaa3",
	"url": "https://github.com/Shaa3"
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true,
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "assets/icons/icon_enabled.svg",
    "default_title": "NoMiners",
    "default_popup": "assets/menu/menu.html"
  },
  "background": {
    "scripts": [
      "script/assets.js",
      "script/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "notifications"
  ],
  "applications": {
    "gecko": {
      "id": "nominer@nominer.com",
      "strict_min_version": "55.0"
    }
  }
}
